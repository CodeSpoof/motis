{
  "name": "rsl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "dev-host": "vite --host",
    "build": "tsc && vite build",
    "serve": "vite preview",
    "serve-host": "vite preview --host",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --max-warnings 0",
    "fix-lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write src",
    "format-check": "prettier --check src",
    "ts-check": "tsc",
    "update-protocol": "node --trace-uncaught tools/protocol.js && eslint src/api/protocol --ext .ts --fix && prettier --write src/api/protocol"
  },
  "dependencies": {
    "@headlessui/react": "^1.7.2",
    "@heroicons/react": "^1.0.6",
    "@radix-ui/react-tooltip": "^1.0.0",
    "@tanstack/react-query": "^4.3.9",
    "@tanstack/react-query-devtools": "^4.3.9",
    "@visx/axis": "^2.12.2",
    "@visx/grid": "^2.12.2",
    "@visx/responsive": "^2.10.0",
    "@visx/scale": "^2.2.2",
    "@visx/shape": "^2.12.2",
    "@visx/stats": "^2.10.0",
    "date-fns": "^2.29.3",
    "downshift": "^6.1.11",
    "jotai": "^1.8.4",
    "lodash-es": "^4.17.21",
    "optics-ts": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.36.0",
    "react-virtuoso": "^2.19.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@limegrass/eslint-plugin-import-alias": "^1.0.6",
    "@tailwindcss/forms": "^0.5.3",
    "@trivago/prettier-plugin-sort-imports": "^3.3.0",
    "@types/lodash-es": "^4.17.6",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "@vitejs/plugin-react": "^2.1.0",
    "autoprefixer": "^10.4.12",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "postcss": "^8.4.16",
    "prettier": "^2.7.1",
    "tailwindcss": "^3.1.8",
    "typescript": "^4.8.3",
    "vite": "^3.1.3"
  }
}
